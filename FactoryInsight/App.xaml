<Application
    x:Class="FactoryInsight.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:FactoryInsight"
    xmlns:localData="using:FactoryInsight.Data"
    xmlns:ioC="using:FactoryInsight.IoC">

    <Application.Resources>
        <!-- Application-specific resources -->

        <x:String x:Key="AppName">FactoryInsight</x:String>

        <ioC:ViewModelLocator x:Key="Locator" />
        <Color x:Key="NoserOrange">#FFFF7900</Color>
    </Application.Resources>
</Application>
